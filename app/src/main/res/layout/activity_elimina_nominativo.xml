                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:layout_marginTop="40dp"
                android:layout_marginBottom="20dp" >

                <Button
                    android:layout_width="120dp"
                    android:layout_height="35dp"
                    android:text="Annulla"
                    android:textColor="@color/bottom_red"
                    android:background="@drawable/register_background"
                    android:layout_alignBottom="@+id/modifica_nominativo_conferma_btn"
                    android:layout_marginRight="20dp"
                    android:id="@+id/elimina_nominativo_annulla_button" />

                <Button
                    android:layout_width="120dp"
                    android:layout_height="35dp"
                    android:text="Elimina"
                    android:id="@+id/elimina_nominativo_conferma_button"
                    android:textColor="@color/blue"
                    android:background="@drawable/access_blue"
                    android:layout_below="@+id/main_register_view"
                    android:layout_marginLeft="25dp" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>
