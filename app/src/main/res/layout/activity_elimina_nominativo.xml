                    android:layout_height="wrap_content"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:id="@+id/elimina_nominativo_societa"
                    android:textColor="@color/asphalt_color"
                    android:singleLine="false"
                    android:layout_marginRight="10dp"
                    android:textStyle="bold" />

                <Button
                    style="?android:attr/buttonStyleSmall"
                    android:layout_width="wrap_content"
                    android:layout_height="25dp"
                    android:background="@drawable/nominativo_societa_view_bg"
                    android:textColor="@color/carot"
                    android:id="@+id/elimina_nominativo_societa_visualizza_button"
                    android:textSize="10dp"
                    android:text="Vedi"
                    android:singleLine="false"
                    android:visibility="invisible"
                    android:layout_marginBottom="5dp" />

            </LinearLayout>

            <LinearLayout
